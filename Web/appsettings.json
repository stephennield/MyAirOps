{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MinimumLevel": "Warning"
    },
    "File": { // Serilog File provider.
      "PathFormat": "Logs/Message-Log-{Date}.txt",
      "LogLevel": {
        "Default": "None", // Overrides preceding LogLevel:Default setting.
        "FileSystemLoggingCategory": "Information" // File:FileSystemLoggingCategory category.
      }
    }
  },
  "AllowedHosts": "*"
}
